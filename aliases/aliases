alias vim="nvim"
alias vi="nvim"
alias v="nvim"
alias vf="fzf -m | xargs nvim -p"
alias vd="nvim -d"
alias vimdiff="nvim -d"
alias tms="tmux-sessionizer"
alias vg="git status -s | awk '{print $2}' | xargs nvim"
alias org="nvim -c 'Neorg index'"
alias cdg="cd `git rev-parse --show-toplevel`"
alias fzf="fzf -m"
alias python="python3"
alias docr="docker-compose down -v --remove-orphans && docker-compose up"
